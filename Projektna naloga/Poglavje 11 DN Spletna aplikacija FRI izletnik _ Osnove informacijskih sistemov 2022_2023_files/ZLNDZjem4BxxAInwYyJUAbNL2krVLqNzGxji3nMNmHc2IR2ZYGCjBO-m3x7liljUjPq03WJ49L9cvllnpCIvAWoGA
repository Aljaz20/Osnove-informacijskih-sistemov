rectangle "FRI izletnik" {
  usecase "**Odjava**\nuporabnika" as OdjavaUporabnika
  usecase "**Pregled**\ndopustov" as PregledDopustov
  usecase "**Pregled**\n**statistike**\nv tabeli" as PregledStatistike
  usecase "**Pregled**\n**dokumentacije**" as PregledDokumentacije
  usecase "**Pregled**\n**načrta**\ninformacijske\nrešitve" as PregledNacrtaIS

  together {
    usecase "**Priprava**\n**računa**" as PripravaRacuna
    usecase "Priprava\n**računa**\nv **HTML**" as PripravaRacunaVHTML
    
    PripravaRacunaVHTML -down-|> PripravaRacuna
  }

  usecase "**Prikaz**\n**podrobnosti**\n**izleta**" as PrikazPodrobnostiIzleta
  usecase "**Uporaba**\n**košarice**\nturističnih\nzanimivosti" as UporabaKosariceTuristicnihZanimivosti
  usecase "**Prikaz**\n**podrobnosti**\nturistične\nzanimivosti" as PrikazPodrobnostiTuristicneZanimivosti

  PrikazPodrobnostiIzleta .up.> PregledDopustov: <<extend>>
  PrikazPodrobnostiIzleta .up.> PripravaRacunaVHTML : <<include>>
  PrikazPodrobnostiTuristicneZanimivosti .up.> UporabaKosariceTuristicnihZanimivosti : <<extend>>

  PregledNacrtaIS -up-|> PregledDokumentacije
}

